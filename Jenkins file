pipeline {
    agent any

    stages {
        stage('Check Out') {
            steps {
                echo 'check out my code'
            }
        }
		    stage('Build') {
            steps {
                echo 'Build my code from git hub'
            }
        }
		    stage('test') {
            steps {
                echo 'test my code'
            }
        }
		    stage('deploy') {
            steps {
                echo 'Deploy my code'
            }
        }
         stage('Notification') {
            steps {
                echo 'Send notification to DevOps Engineer'
            }
        }
    }
}

