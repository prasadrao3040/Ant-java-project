pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                echo 'take code from github'
            }
        }
        stage('Build') {
            steps {
                echo 'Build my project'
            }
        }
         stage('Test') {
            steps {
                echo 'test my project'
            }
        }
         stage('Deploy') {
            steps {
                echo 'test my project'
            }
        }
    }
}
